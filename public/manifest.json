{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Valorant Utils",
    "version": "0.1.0",
    "minimum-overwolf-version": "0.120.0",
    "author": "Hacki",
    "icon": "IconMouseOver.png",
    "icon_gray": "IconMouseNormal.png",
    "launcher_icon": "desktop-icon.ico",
    "window_icon": "TaskbarIcon.png",
    "description": "Testing Overwolf Application Developement"
  },
  "permissions": [
    "Extensions",
    "Profile",
    "Hotkeys",
    "GameInfo"
  ],
  "data": {
    "hotkeys": {
      "show_ingame": {
        "title": "Show Ingame Menu",
        "action-type": "custom",
        "default": "Ctrl+F5",
        "passthrough": true
      },
      "show_cat": {
        "title": "Show Ingame Menu",
        "action-type": "custom",
        "default": "F5",
        "passthrough": true,
        "hold": true
      }
    },
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [
        21640
      ]
    },
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [
            21640
          ]
        },
        "start_minimized": true
      }
    ],
    "game_events": [
      21640
    ],
    "start_window": "background",
    "windows": {
      "background": {
        "file": "Files/index.html",
        "background_optimization": false,
        "is_background_page": true
      },
      "desktop": {
        "file": "Files/index.html",
        "native_window": true,
        "transparent": true,
        "size": {
          "width": 1200,
          "height": 600
        },
        "min_size": {
          "width": 900,
          "height": 400
        },
        "max_size": {
          "width": 1920,
          "height": 1080
        },
        "desktop_only": true,
        "block_top_window_navigation": true,
        "popup_blocker": true,
        "mute": true
      },
      "in_game": {
        "file": "Files/index.html",
        "in_game_only": true,
        "focus_game_takeover": "ReleaseOnHidden",
        "focus_game_takeover_release_hotkeys": [
          "show_Ingame"
        ],
        "block_top_window_navigation": true,
        "transparent": true,
        "resizable": true,
        "size": {
          "width": 1920,
          "height": 1080
        },
        "min_size": {
          "width": 0,
          "height": 0
        },
        "max_size": {
          "width": 1920,
          "height": 1080
        },
        "start_position": {
          "top": 0,
          "left": 0
        }
      }
    },
    "force_browser": "user",
    "developer": {
      "enable_auto_refresh": true,
      "reload_delay": 1000,
      "filter": "*.*"
    }
  }
}